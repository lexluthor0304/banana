{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "types": ["@cloudflare/workers-types"],
    "resolveJsonModule": true
  },
  "include": ["src", "functions"],
  "exclude": ["node_modules", "dist"]
}
